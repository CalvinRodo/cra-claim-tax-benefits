extends ../base

block variables
  -var title = __('Deduct your Registered Retirement Savings Plan (RRSP) contributions')
  -var rrspClaim = hasData(data, 'deductions.rrspClaim') ? data.deductions.rrspClaim : ''

block content

  h1 #{title}

  form.cra-form(method='post')
    +radiosYesNo('rrspClaim', 'Did you contribute to a Registered Retirement Savings Plan (RRSP) before March 1, 2019?', rrspClaim)

    input#redirect(name='redirect', type='hidden', value='/personal/maritalStatus')

    +formButtons()
